package <%= @package %>;
<% if not @imports.nil? 
@imports.each do |import| %>
import <%= import %>;<% end 
end %>

/**
 * 
 * 
 * @author <%= @name %>
 */
public class <%= @class_name %> {
<% if not @fields.nil? 
@fields.each do |field| %>
    private <%= field.type %> _<%= field.name %>;<% end 
end %>

    public <%= @class_name %>() {
        super();
    }
<% if not @fields.nil?
@fields.each do |field|
if field.getter_and_setter %>
    public <%= field.type %> get<%= field.name.capitalize_first %>() {
        return _<%= field.name %>;
    }

    public void set<%= field.name.capitalize_first %>(<%= field.type %> <%= field.name %>) {
        _<%= field.name %> = <%= field.name %>;
    }
<% end
end
end %>
}
